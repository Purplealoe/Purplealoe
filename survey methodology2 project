        addMarkers(lng=~경도, lat=~위도, label=~사업장명22)
      
    }else if(input$업종=="전체"&input$BEST=="전체"){
      leaflet(sb2) %>%
        setView(lng=126.9784, lat=37.55, zoom=12) %>%
        addTiles() %>%
        addMarkers(lng=~경도, lat=~위도, label=~사업장명22)
      
      #전체
    }else if(input$업종=="전체"&input$BEST=="임금"){
      leaflet(subset(sb2,  임금22 == 1 & 임금21==1)) %>%
        setView(lng=126.9784, lat=37.55, zoom=12) %>%
        addTiles() %>%
        addMarkers(lng=~경도, lat=~위도, label=~사업장명22)
      
    }else if(input$업종=="전체"&input$BEST=="일생활균형"){
      leaflet(subset(sb2,  일생활균형22 == 1 & 일생활균형21== 1)) %>%
        setView(lng=126.9784, lat=37.55, zoom=12) %>%
        addTiles() %>%
        addMarkers(lng=~경도, lat=~위도, label=~사업장명22)
      
    }else if(input$업종=="전체"&input$BEST=="고용안정"){
      leaflet(subset(sb2,  고용안정22 == 1 & 고용안정21== 1)) %>%
        setView(lng=126.9784, lat=37.55, zoom=12) %>%
        addTiles() %>%
        addMarkers(lng=~경도, lat=~위도, label=~사업장명22)
      
    }else if(input$업종=="전체"&input$BEST=="임금&일생활균형"){
      leaflet(subset(sb2,  임금22 == 1 & 일생활균형22==1 & 임금21 == 1 & 일생활균형21 == 1)) %>%
        setView(lng=126.9784, lat=37.55, zoom=12) %>%
        addTiles() %>%
        addMarkers(lng=~경도, lat=~위도, label=~사업장명22)
      
    }else if(input$업종=="전체"&input$BEST=="임금&고용안정"){
      leaflet(subset(sb2,  임금22 == 1 & 고용안정22==1 & 임금21 == 1 & 고용안정21 == 1)) %>%
        setView(lng=126.9784, lat=37.55, zoom=12) %>%
        addTiles() %>%
        addMarkers(lng=~경도, lat=~위도, label=~사업장명22)   
      
    }else if(input$업종=="전체"&input$BEST=="일생활균형&고용안정"){
      leaflet(subset(sb2,  일생활균형22 == 1 & 고용안정22==1 & 일생활균형21 == 1 & 고용안정21 == 1)) %>%
        setView(lng=126.9784, lat=37.55, zoom=12) %>%
        addTiles() %>%
        addMarkers(lng=~경도, lat=~위도, label=~사업장명22)
      
    }else if(input$업종=="전체"&input$BEST=="임금&일생활균형&고용안정"){
      leaflet(subset(sb2,  랭킹 == 1)) %>%
        setView(lng=126.9784, lat=37.55, zoom=12) %>%
        addTiles() %>%
        addMarkers(lng=~경도, lat=~위도, label=~사업장명22)
      
    }
    
    
  })
  
  
  output$map4 <- renderLeaflet({
