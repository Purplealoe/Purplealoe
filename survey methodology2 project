    #정보통신업
    if(input$업종=="정보통신업"&input$BEST=="임금"){
      leaflet() %>%
        setView(lng=126.9784, lat=37.55, zoom=12) %>%
        addTiles() %>%
        addAwesomeMarkers(data = a1,lng=a1$경도, lat=a1$위도, popup=paste0("사업장명: ", a1$사업장명, "<br/>소재지: ", a1$소재지, "<br/>기업규모: ", a1$기업규모))
      
    }else if(input$업종=="정보통신업"&input$BEST=="일생활균형"){
